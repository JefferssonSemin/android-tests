<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/appFragment">

    <fragment
        android:id="@+id/appFragment"
        android:name="com.hvn.ci.ui.AppFragment"
        android:label="AppFragment">

        <action
            android:id="@+id/action_appFragment_to_nav_usuario"
            app:destination="@id/nav_usuario" />
        <action
            android:id="@+id/action_appFragment_to_nav_home"
            app:destination="@id/nav_home" />

    </fragment>

    <include app:graph="@navigation/nav_home" />
    <include app:graph="@navigation/nav_usuario" />
</navigation>